 Cell[BoxData[
  FormBox[
   SubscriptBox["z", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "84e9d7dd-335c-484c-b758-115dd6fa3c7c"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["z", "17"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e634c0db-ddb7-4caa-a5d9-7df30171ddaa"],
 ",  for the 807 SNP that lie in windows with  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox[
     SuperscriptBox["\[CapitalDelta]z", "2"], "_"], ">", "2"}], 
   TraditionalForm]],ExpressionUUID->"a133791f-84cc-4d23-9e22-4a4744079cd5"],
 " (blue: LS1; red: LS2)."
}], "Text",
