   RowBox[{"Options", "[", "SparsePedigreeMatrix", "]"}], "=", 
   RowBox[{"{", 
    RowBox[{"Map", "\[Rule]", "\[Infinity]"}], "}"}]}], ";"}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.667470015181666*^9, 3.6674700264683104`*^9}, 
   3.667470057554564*^9},
